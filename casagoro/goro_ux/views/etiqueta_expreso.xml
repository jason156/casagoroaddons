<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<template id="delivery_label">
		<t t-call="web.html_container">
			<div class="page">
				<t t-as="o" t-foreach="docs">
					<t t-as="i" t-foreach="range(o.number_of_packages)">
						<div style="height:130mm;border:dotted 1px #000;padding:3px; page-break-inside: avoid;">
							<div style="padding:15px;">
								<div style="height:40mm;">
									<img src="data:image/jpeg;base64," style="float:right;height:40mm;"/>
									<h1 t-field="o.partner_id.name"/>
									<div style="font-size:1.5em;">
										<span t-field="o.partner_id.street"/>
										<span t-field="o.partner_id.street2"/>
									</div>
									<div style="font-size:1.5em;">
										<span t-field="o.partner_id.city"/>
										<span t-field="o.partner_id.state_id.name"/>(
										<span t-field="o.partner_id.zip"/>
										)
										<br/>
										<span t-field="o.partner_id.country_id.name"/></div>
								</div>
								<div style="padding:15px; text-align:right;">
									<h2>Rte: Casa Goro</h2>
									<div>Avenida Provincias Unidas 2415</div>
									<div>Rosario Santa Fe S2009 </div>
									<div>Argentina</div>
								</div>
							</div>
							<div style="background-color:#000; color:#FFF; padding:15px;">
								<h2>Bulto
									<span t-esc="i + 1 "/>
									/ 
									<span t-field="o.number_of_packages"/></h2>
									<div t-if="o.carrier_id and o.carrier_id.partner_id">Envio por: <strong t-field="o.carrier_id.partner_id.name"/>
										<span t-field="o.carrier_id.partner_id.street"/>
										<span t-field="o.carrier_id.partner_id.street2"/>
										<span t-field="o.carrier_id.partner_id.city"/>
										<span t-field="o.carrier_id.partner_id.state_id.name"/></div>

							</div>
						</div>
					</t>
				</t>
			</div>
		</t>
	</template>
	<report id="report_delivery_label" model="stock.picking" name="goro_ux.delivery_label" report_type="qweb-pdf" string="Transporte Goro"/>
    <record id="report_delivery_label" model="ir.actions.report">
        <field name="paperformat_id" ref="watch_products.paperformat_a4label"/>
    </record>
