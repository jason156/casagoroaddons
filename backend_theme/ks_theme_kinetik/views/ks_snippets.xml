<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <template id="snippet_options" inherit_id="website.snippets">
        <xpath expr="//div[@id='snippet_options']" position="inside">
            <div data-selector="img">
                <li data-toggle-class="s"
                    data-no-preview="true"
                    title="Disable lazy loading for this image">
                    <a>
                        Disable Lazy loading
                    </a>
                </li>
            </div>
        </xpath>
    </template>


    <template id="place_into_bar" inherit_id="website.snippets" name="Place into bar">
        <xpath expr="//div[@id='snippet_structure']" position="before">
            <div class="o_panel_header">
                <i class="fa fa-th-large"/>
                <b>Kinetik Snippets</b>
            </div>

            <div class="o_panel_header">
                <i class="fa fa-star"/>
                Dynamic Snippets
            </div>
            <div class="o_panel_body">
                <t t-snippet="ks_theme_kinetik.snippet_blogs_home_page"
                   t-thumbnail="/ks_theme_kinetik/static/src/img-ui/snippets_icons/blogs.png"/>
                <t t-snippet="ks_theme_kinetik.snippet_brands_home_page"
                   t-thumbnail="/ks_theme_kinetik/static/src/img-ui/snippets_icons/brands.png"/>
                <t t-snippet="ks_theme_kinetik.snippet_featured_home_page"
                   t-thumbnail="/ks_theme_kinetik/static/src/img-ui/snippets_icons/featured.png"/>
                <t t-snippet="ks_theme_kinetik.snippet_product_grid"
                   t-thumbnail="/ks_theme_kinetik/static/src/img-ui/snippets_icons/product_grid.png"/>
                <t t-snippet="ks_theme_kinetik.ks_trendy_style_static"
                   t-thumbnail="/ks_theme_kinetik/static/src/img-ui/snippets_icons/trendy.png"/>
                <!--                <t t-snippet="ks_theme_kinetik.snippet_offer_timer"-->
                <!--                   t-thumbnail="/ks_theme_kinetik/static/src/img-ui/snippets_icons/offer-snippet.png"/>-->
                <t t-snippet="ks_theme_kinetik.snippet_recently_viewed_products"
                   t-thumbnail="/ks_theme_kinetik/static/src/img-ui/snippets_icons/offer-snippet.png"/>
                <t t-snippet="ks_theme_kinetik.ks_header_offer_snippet"
                   t-thumbnail="/ks_theme_kinetik/static/src/img-ui/snippets_icons/header_001.png"/>
                <t t-snippet="ks_theme_kinetik.ks_video_static"
                   t-thumbnail="/ks_theme_kinetik/static/src/img-ui/snippets_icons/video.png"/>
            </div>
        </xpath>
    </template>

