    <template id="ks_theme_kinetik.products_attributes" inherit_id="website_sale.products_attributes"
              name="Kinetik Attribute's, Brand's and Price's Filters" priority="100">
        <xpath expr="//div[@id='wsale_products_attributes_collapse']" position="replace">
            <form class="js_attributes ks-filter-outer" method="get">
                 <input type="hidden" name="ppg" class="ks_text" t-att-value="ppg"/>
                <input type="hidden" class="hidden_search_field" name="search" t-att-value="search"/>
                <ul class="nav nav-pills flex-column mt-3">
<!--                    <t t-foreach="brands" t-as="b">-->
                    <div class="align-items-center justify-content-between ks-filter-brand-menu py-2"
                         data-toggle="collapse" href="#ksBrandContainer" role="button" aria-expanded="false"
                         aria-controls="ksBrandContainer">
                        <strong>Brands</strong>
                        <i class="fa fa-chevron-down "/>
                    </div>
<!--                    </t>-->
<!--                    <t t-if="a.display_type == 'radio'">-->
                    <ul class="list-unstyled w-100 collapse ml-0 pl-4" id="ksBrandContainer">
                        <t t-foreach="brands" t-as="b">
                            <li class="nav-item">
                                <label t-attf-class="nav-link#{' ks_brand_active active' if b.id in brnd_set else ''}">
                                    <input data-oe-id="b.id" type="checkbox" name="brnd"
                                           t-att-value="'%s-%s' % (0,b.id)"
                                           t-att-checked="'checked' if b.id in brnd_set else None"/>
                                    <span style="font-weight: normal" t-field="b.name"/>
                                </label>
                            </li>
                        </t>
                    </ul>
<!--                    </t>-->
                    <t t-foreach="attributes" t-as="a">
                        <li t-if="a.value_ids and len(a.value_ids) &gt; 1" class="nav-item">
                            <div class="align-items-center justify-content-between ks-filter-brand-menu"
                                 data-toggle="collapse" t-att-href="'#collapseElementId'+str(a.id)" role="button"
                                 aria-expanded="false" t-att-aria-controls="'collapseElementId'+str(a.id)">
                                <strong t-field="a.name"/>
                                <i class="fa fa-chevron-down ks-filter-transition"/>
                            </div>
                            <t t-if="a.display_type == 'select'">
                                <div class="collapse" t-att-id="'collapseElementId'+str(a.id)">
                                    <select class="ks_select_attrib form-control" name="attrib">
                                        <option value=""/>
                                        <t t-foreach="a.value_ids" t-as="v">
                                            <option t-att-value="'%s-%s' % (a.id,v.id)" t-esc="v.name" data-oe-id="a.id"
                                                    t-att-selected="v.id in attrib_set"/>
                                        </t>
                                    </select>
                                </div>
                            </t>
                            <t t-if="a.display_type == 'radio'">
                                <ul class="nav nav-pills flex-column collapse" t-att-id="'collapseElementId'+str(a.id)">
                                    <t t-foreach="a.value_ids" t-as="v">
                                        <li class="nav-item w-100">
                                            <label t-attf-class="nav-link#{' ks_attrib_active active' if v.id in attrib_set else ''}">
                                                <input type="checkbox" name="attrib" t-att-value="'%s-%s' % (a.id,v.id)"
                                                       t-att-checked="'checked' if v.id in attrib_set else None"
                                                       data-oe-id="a.id"/>
                                                <span style="font-weight: normal" t-field="v.name"/>
                                            </label>
                                        </li>
                                    </t>
                                </ul>
                            </t>
                            <t t-if="a.display_type == 'color'">
                                <div class="collapse" t-att-id="'collapseElementId'+str(a.id)">
                                    <t t-foreach="a.value_ids" t-as="v">
                                        <label t-attf-style="background-color:#{v.html_color or v.name}"
                                               t-attf-class="css_attribute_color #{' ks_attrib_active active' if v.id in attrib_set else ''}">
                                            <input type="checkbox" name="attrib" data-oe-id="a.id"
                                                   t-att-value="'%s-%s' % (a.id,v.id)"
                                                   t-att-checked="'checked' if v.id in attrib_set else None"
                                                   t-att-title="v.name"/>
                                        </label>
                                    </t>
                                </div>
                            </t>
                        </li>
                    </t>
                    <!--Price Filter Container-->
                    <div class="container">
                        <label for="amount" class="font-weight-normal">Price Range</label>
                        <div class="w-100 d-flex justify-content-center">

                            <span id="ks-price-filter" type="text" class="span2" value=""
                                  t-att-data-slider-min="float(min_price_set)"
                                  t-att-data-slider-max="float(max_price_set)"
                                  data-slider-step="5"
                                  t-att-data-slider-value="[float(min_price_selected),float(max_price_selected)]"/>
                            <div class="d-flex justify-content-between w-100 my-2">
                                <span>
                                    <t t-esc="currency_symbol"></t>
                                    <t t-esc="min_price_set"></t>
                                </span>
                                <span>
                                    <t t-esc="currency_symbol"></t>
                                    <t t-esc="max_price_set"></t>
                                </span>
                            </div>
                        </div>
                        <div class="w-100 row justify-content-center mb-3">
                               <input id="ks-selected_input_min" class="w-50 col-4"
                                       t-att-value="min_price_selected if min_price_selected else '0'" disabled="true"></input>
                                <input id="ks-selected_input_min_hidden" name="min" type="hidden"
                                       t-att-value="min_price_selected if min_price_selected else '0.0'"></input>
                                <input id="ks-selected_input_max" class="w-50 col-6"
                                       t-att-value="max_price_selected" disabled="true"></input>
                                <input id="ks-selected_input_max_hidden" name="max" type="hidden"
                                       t-att-value="float(max_price_selected)"></input>
                        </div>
                        <input type="submit" name="filter_clear" value="Apply Filters"
                               class="btn btn-primary w-100 ks-price-filter-apply mb-4"/>
                    </div>
                </ul>
            </form>
        </xpath>
        <xpath expr="//div[@id='products_grid_before']" position="attributes">
            <attribute name="class">col-12</attribute>
        </xpath>
        <xpath expr="//div[@id='products_grid']" position="attributes">
            <attribute name="class">col-lg-9</attribute>
        </xpath>
    </template>
