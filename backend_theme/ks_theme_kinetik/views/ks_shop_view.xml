    <template id="ks_theme_kinetik.sort" inherit_id="website_sale.sort"
              name="Show Sort by">
        <xpath expr="//div[hasclass('dropdown_sorty_by')]/div//a" position="inside">
            <input class="d-none ks_sortable" t-att-value="sortby[1]"/>
        </xpath>
    </template>
    <template id="website_sale.categories_recursive" name="Category list">
        <li t-attf-class="nav-item ks-filter-main #{' ks-remove-chevron' if not c.child_id else ''}">
            <div class="align-items-center justify-content-between ks-filter-head">
                <a t-att-href="keep('/shop/category/' + slug(c), category=0)"
                   t-attf-class="nav-link#{' ks_active active' if c.id == int(category or 0) else ''}">
                    <span t-field="c.name"/>
                </a>
                <i class="fa fa-chevron-down" data-toggle="collapse" t-att-href="'#collapseId'+str(c.id)" role="button"
                   aria-expanded="false" t-att-aria-controls="'collapseId'+str(c.id)"/>
            </div>
            <ul t-if="c.child_id" class="nav nav-pills flex-column nav-hierarchy ks-filter-open collapse"
                t-att-id="'collapseId'+str(c.id)">
                <t t-foreach="c.child_id" t-as="c">
                    <t t-if="not search or c.id in search_categories_ids">
                        <t t-call="website_sale.categories_recursive"/>
                    </t>
                </t>
            </ul>
        </li>
    </template>
    <!--This template is used to design shop page product layout better with designing-->
    <template id="ks_change_b_tag" name="ks tag b" inherit_id="website_sale.products_item">
        <xpath expr="//form[1]" position="replace">
            <form action="/shop/cart/update" method="post">
                <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>
                <div itemscope="itemscope" itemtype="http://schema.org/Product">
                    <div class="ribbon-wrapper">
                        <a href="#" role="button" class="ribbon btn btn-danger">Sale</a>
                    </div>
                        <div class="ks-product-list-mode">
                            <div class="oe_product_image">
                                <a itemprop="url" t-att-href="product_href">
                                    <span t-field="product.image_1920"
                                           t-options="{'widget': 'image', 'preview_image': 'image_1024' if product_image_big else 'image_256'}"
                                         class="d-flex h-100 justify-content-center align-items-center"/>
                                </a>
                            </div>
                            <t t-if="show_publish">
                            <div id="website_published_button" class="" groups="sales_team.group_sale_manager">
                                <t t-call="website.publish_management">
                                    <t t-set="object" t-value="product"/>
                                    <t t-set="publish_edit" t-value="True"/>
                                    <t t-set="action" t-value="'product.product_template_action'"/>
                                </t>
                            </div>
                        </t>
                            <section class="ks-product-content" style="background: transparent;">
                                <div class="ks_name_price">
<!--                                    <h5>-->
<!--                                        <t t-esc="product.name"/>-->
<!--                                    </h5>-->
                                    <div class="fp__name text-truncate">
                                        <t t-esc="product.name"/>
                                    </div>
                                    <t t-if="request.website.viewref('ks_theme_kinetik.ks_product_comment').active">
                                        <t t-call="website_rating.rating_widget_stars_static" >
                                            <t t-set="rating_avg" t-value="product.rating_avg"/>
                                            <t t-set="rating_count" t-value="product.rating_count"/>
                                        </t>
                                    </t>
                                    <t t-if='product.ks_product_brand_id.name'>
                                        <div class="ks-product-sub">
                                            <t t-esc="product.ks_product_brand_id.name"/>
                                        </div>
                                    </t>
<!--                                    <t t-call="website_sale.product_price"/>-->
                                    <div class="ks-price">
                                        <t t-if="True"><!-- compatibility 12.0 -->
                                            <del t-attf-class="text-danger mr8 {{'' if combination_info['has_discounted_price'] else 'd-none'}}"
                                                 style="white-space: nowrap;" t-esc="combination_info['list_price']"
                                                 t-options="{'widget': 'monetary', 'display_currency': website.currency_id}"/>
                                        </t>
                                        <span t-esc="combination_info['price']"
                                              t-options="{'widget': 'monetary', 'display_currency': website.currency_id}"/>
                                        <span itemprop="price" style="display:none;" t-esc="combination_info['price']"/>
                                        <span itemprop="priceCurrency" style="display:none;" t-esc="website.currency_id.name"/>
                                    </div>
                                </div>
                                 <div class="ks_hover_data">
                                    <div itemprop="offers" itemscope="itemscope" itemtype="http://schema.org/Offer"
                                         class="product_price">
                                         <div class="o_wsale_product_btn"/>
                                    </div>

                                </div>

                            </section>
                    </div>
                </div>
            </form>
        </xpath>

    </template>


