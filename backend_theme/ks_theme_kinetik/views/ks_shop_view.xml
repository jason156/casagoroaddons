<odoo>
    <!--is used to place filter button with condition if filter arttribute on it will display&ndash;&gt;-->
   <template id="ks_product_pager" inherit_id="website_sale.products" priority="0" name=" filter button">
        <xpath expr="//div[hasclass('products_pager')]" position="replace">
            <div class="ks_pager_container">
                <div class="products_pager form-inline justify-content-center">
                    <t t-call="website_sale.search">
                        <t t-set="_classes">o_website_sale_search</t>
                    </t>
                    <t t-call="website_sale.pricelist_list">
                        <t t-set="_classes">ml-2</t>
                    </t>
                    <t t-call="website.pager">
                        <t t-set="_classes">ml-2</t>
                    </t>
                </div>
            </div>
        </xpath>
    </template>
    <template id="ks_filter_button" inherit_id="website_sale.products" name=" filter button">
        <xpath expr="//div[hasclass('products_pager')]/t[@t-call][last()]" position="before">
            <t t-if="request.website.viewref('website_sale.products_categories').active or request.website.viewref('website_sale.products_attributes').active">
                <div class="ks_filter_button btn btn-secondary mr-2">
                    <i class="fa fa-filter mr-md-2"></i>
                    <span class="text-filter">Filter</span>
                </div>
                <div class="ks-loader d-none"></div>
            </t>
        </xpath>
        <xpath expr="//div[@id='products_grid']" position="before">
            <div class="ks-filter-overlay"/>
        </xpath>
    </template>
    <template id="ks_Product_not_fnd" inherit_id="website_sale.products" name="product not found">
        <xpath expr="//div[hasclass('text-muted')]//h3" position="replace">
             <t t-if="request.website.viewref('website_sale.products_categories').active or request.website.viewref('website_sale.products_attributes').active ">
             <div class="d-none filter-selectedFilterContainer">
                   <div class="brand_filter_list">
                       <label class="brand_filter d-none">Brand:</label>
                   </div>
                   <div class="variants_filter_list">
                         <label class="variants_filter d-none">Variants:</label>
                   </div>
                 <div class="price_filter_list">
<!--                         <label class="price_filter_shop d-none">Price:</label>-->
                   </div>
                   <div class="filterList">

                   </div>
                 <div class="d-inline-block">
                     <button class="ks-search-clear">Clear All Filters</button>
                 </div>
               </div>
              </t>
             <h3 class="css_editable_display mt8">No Product Found.</h3>
        </xpath>
         <xpath expr="//div[hasclass('text-muted')]/t[last()]/h3" position="before">
             <t t-if="request.website.viewref('website_sale.products_categories').active or request.website.viewref('website_sale.products_attributes').active ">
             <div class="d-none filter-selectedFilterContainer">
                   <div class="brand_filter_list">
                       <label class="brand_filter d-none">Brand:</label>
                   </div>
                   <div class="variants_filter_list">
                         <label class="variants_filter d-none">Variants:</label>
                   </div>
                 <div class="price_filter_list">
<!--                         <label class="price_filter_shop d-none">Price:</label>-->
                   </div>
                   <div class="filterList">

                   </div>
                 <div class="d-inline-block">
                     <button class="ks-search-clear">Clear All Filters</button>
                 </div>
               </div>
              </t>
        </xpath>
    </template>
    <template id="website_sale.products_categories" inherit_id="website_sale.products" active="False" customize_show="True" name="eCommerce Categories">
          <xpath expr="//div[@id='products_grid_before']" position="before">
            <t t-set="enable_left_column" t-value="True"/>
        </xpath>
        <xpath expr="//div[@id='products_grid_before']" position="inside">
            <div class="ks_product_categories_before" data-toggle="collapse" href="#ks_categories_container" role="button" aria-expanded="false" aria-controls="ks_categories_container">
                 <span>Categories</span><i class="fa fa-chevron-down " />
             </div>
            <ul class="nav nav-pills flex-column mt16 collapse" id="ks_categories_container">
                <li class="nav-item">
                    <a t-att-href="keep('/shop',category=0)" t-attf-class="nav-link#{'' if category else ' active'} o_not_editable">All Products</a>
                </li>
                <t t-foreach="categories" t-as="c">
                    <t t-call="website_sale.categories_recursive" />
                </t>
            </ul>
        </xpath>
        <xpath expr="//div[@id='products_grid_before']" position="attributes">
            <attribute name="class">col-lg-3 col-md-4 col-12</attribute>
        </xpath>
        <xpath expr="//div[@id='products_grid']" position="attributes">
            <attribute name="class">col-lg-9 col-md-8</attribute>
        </xpath>
    </template>
