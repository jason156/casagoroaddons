    <template inherit_id='website_sale.products' id="website_sale.add_grid_or_list_option" customize_show="False"/>

    <template id="ks_product_per_page" inherit_id="website_sale.products" name="Show Product Per page">
      <xpath expr="//div[hasclass('products_pager')]/t[@t-call][last()]" position="after">
          <t t-if="request.website.viewref('website_sale.products_attributes').active">
           <div class="product-item_qty-per-page">
                            <label class="product-label">Show</label>
                            <select class="product-select pl-2">
                                <t t-if="ppg==12">
                                    <option value="12" selected="selected">12</option>
                                    <option value="24">24</option>
                                    <option value="36">36</option>
                                    <option value="44">44</option>
                                  </t>
                                <t t-if="ppg==24">
                                    <option value="12">12</option>
                                    <option value="24" selected="selected">24</option>
                                    <option value="36">36</option>
                                    <option value="44">44</option>
                                </t>
                                <t t-if="ppg==36">
                                    <option value="12">12</option>
                                    <option value="24">24</option>
                                    <option value="36" selected="selected">36</option>
                                    <option value="44">44</option>
                                </t>
                                <t t-if="ppg==44">
                                    <option value="12">12</option>
                                    <option value="24">24</option>
                                    <option value="36">36</option>
                                    <option value="44" selected="selected">44</option>
                                </t>
                            </select>
               <form action="/shop" method="get">
                        <input type="hidden" name="ppg" class="ks_text" t-att-value="ppg"/>
                        <input type="hidden" name="category" t-att-value="category"/>
                         <t t-if="order!=None">
                            <input  type="hidden" name="order" class="ks_sort_per_page" t-att-value="order"/>
                           </t>
               </form>
            </div>
           </t>
     </xpath>
    </template>

     <template id="load_more" inherit_id="website_sale.products">
          <xpath expr="//div[@id='products_grid']" position="after">
              <div class="d-none display_load_more justify-content-center mt-2">
                  <t t-if="bins">
                  <button class="product_load_more btn btn-secondary">Load More</button>
                  <input type="hidden" name="ppg" class="page_number" t-att-value="ppg"></input>
                   <input type="hidden" name="page_count" class="page_count" t-att-value="page_count"></input>
                  <t t-if="category_name!=None">
                   <input type="hidden" name="category" class="category_active" t-att-value="category_name"/>
                  </t>
                   <t t-if="order!=None">
                    <input  type="hidden" name="order" class="ks_sort_per_page" t-att-value="order"/>
                   </t>
                   <input id="ks-selected_input_min_hidden" name="min" type="hidden"
                                   t-att-value="float(min_price_selected)"></input>
                   <input id="ks-selected_input_max_hidden" name="max" type="hidden"
                                   t-att-value="float(max_price_selected)"></input>
                   <input type="hidden" class="hidden_search_field" name="search" t-att-value="search"/>
                  </t>
              </div>
          </xpath>
     </template>
     <template id="ks_theme_kinetik.products_infinite_loader">
         <t t-foreach="bins" t-as="tr_product">
                        <t t-foreach="tr_product" t-as="td_product">
                            <t t-if="td_product">
                                <t t-set="product" t-value="td_product['product']"/>
                                 <div class="col col-6 col-md-6 col-lg-3 ks-product-list">
                                    <div t-att-colspan="td_product['x'] != 1 and td_product['x']"
                                        t-att-rowspan="td_product['y'] != 1 and td_product['y']"
                                        t-attf-class="oe_product oe_grid oe-height-#{td_product['y']*2} #{ td_product['class'] }">
                                        <div class="oe_product_cart"
                                             t-att-data-publish="product.website_published and 'on' or 'off'">
                                            <t t-set="product_image_big"
                                               t-value="td_product['x']+td_product['y'] &gt; 2"/>
                                            <t t-call="website_sale.products_item"/>
                                        </div>
                                    </div>
                                </div>
                            </t>
                            <td t-if="not td_product" class="oe-height-2"/>
                        </t>
                    </t>
                <t t-if="not bins">
                    <div class="text-center text-muted oe_product">
                        <h3 class="css_editable_display mt8">No Product Found.</h3>
                        <t t-if="search">
                            <p>
                                No results found for "<strong t-esc='search'/>".
                            </p>
                        </t>
                        <p groups="sales_team.group_sale_manager">Click <i>'New'</i> in the top-right corner to create
                            your
                            first product.
                        </p>
                    </div>
                </t>
        </template>

    <template id="ks_theme_kinetik.products_list_view_load_more">
                <t t-foreach="products" t-as="product">
                    <div class="oe_product oe_list oe_product_cart"
